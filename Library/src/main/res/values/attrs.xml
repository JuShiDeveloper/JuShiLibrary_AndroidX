<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ClipViewLayout">
        <attr name="clipType" format="enum">
            <enum name="circle" value="1" />
            <enum name="rectangle" value="2" />
        </attr>
        <attr name="mHorizontalPadding" format="dimension" />
        <attr name="clipBorderWidth" format="dimension" />
    </declare-styleable>
    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_fill_color" format="color" />
    </declare-styleable>

    <declare-styleable name="LottieAnimationView">
        <attr name="lottie_fileName" format="string" />
        <attr name="lottie_rawRes" format="reference" />
        <attr name="lottie_url" format="string" />
        <attr name="lottie_autoPlay" format="boolean" />
        <attr name="lottie_loop" format="boolean" />
        <attr name="lottie_repeatMode" format="enum">
            <enum name="restart" value="1" />
            <enum name="reverse" value="2" />
        </attr>
        <attr name="lottie_repeatCount" format="integer" />
        <attr name="lottie_imageAssetsFolder" format="string" />
        <attr name="lottie_progress" format="float" />
        <attr name="lottie_enableMergePathsForKitKatAndAbove" format="boolean" />
        <attr name="lottie_colorFilter" format="color" />
        <attr name="lottie_scale" format="float" />
        <attr name="lottie_speed" format="float" />
        <!-- These values must be kept in sync with the RenderMode enum -->
        <attr name="lottie_renderMode" format="enum">
            <enum name="automatic" value="0" />
            <enum name="hardware" value="1" />
            <enum name="software" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="PagerSlidingTabStrip">
        <attr name="pstsIndicatorColor" format="color" />
        <attr name="pstsUnderlineColor" format="color" />
        <attr name="pstsDividerColor" format="color" />
        <attr name="pstsIndicatorHeight" format="dimension" />
        <attr name="pstsUnderlineHeight" format="dimension" />
        <attr name="pstsDividerPaddingTopBottom" format="dimension" />
        <attr name="pstsTabPaddingLeftRight" format="dimension" />
        <attr name="pstsScrollOffset" format="dimension" />
        <attr name="pstsTabBackground" format="reference" />
        <!--该属性表示里面的TAB是否均分整个PagerSlidingTabStrip控件的宽,true是,false不均分,从左到右排列,默认false-->
        <attr name="pstsShouldExpand" format="boolean" />
        <attr name="pstsTextAllCaps" format="boolean" />
        <attr name="pstsTextSelectedColor" format="color" />
        <!--缩放的最大值,0.3表示放大后最大是原来的0.3倍,默认未0.3-->
        <attr name="pstsScaleZoomMax" format="float" />
        <attr name="pstsSmoothScrollWhenClickTab" format="boolean" />
        <!-- 宽为match_parent ，指示器的宽需手动计算的问题,这个属性设置为true,则指示器的位置和字体的宽一样-->
        <attr name="pstsIndicatorWrap" format="boolean" />
    </declare-styleable>

    <declare-styleable name="SelectableRoundedImageView">
        <attr name="riv_corner_radius" format="dimension" />
        <attr name="sriv_left_top_corner_radius" format="dimension" />
        <attr name="sriv_right_top_corner_radius" format="dimension" />
        <attr name="sriv_left_bottom_corner_radius" format="dimension" />
        <attr name="sriv_right_bottom_corner_radius" format="dimension" />
        <attr name="sriv_border_width" format="dimension" />
        <attr name="sriv_border_color" format="color" />
        <attr name="riv_mutate_background" format="boolean" />
        <attr name="sriv_oval" format="boolean" />
        <attr name="android:scaleType" />
        <attr name="riv_tile_mode">
            <enum name="clamp" value="0" />
            <enum name="repeat" value="1" />
            <enum name="mirror" value="2" />
        </attr>
        <attr name="riv_tile_mode_x">
            <enum name="clamp" value="0" />
            <enum name="repeat" value="1" />
            <enum name="mirror" value="2" />
        </attr>
        <attr name="riv_tile_mode_y">
            <enum name="clamp" value="0" />
            <enum name="repeat" value="1" />
            <enum name="mirror" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="CustomEditText">
        <attr name="digits" format="string|reference" /> <!--定义可输入字符属性，与EditText的属性使用相同-->
        <attr name="drawableLeft" format="reference" />  <!--与EditText的drawableLeft属性使用相同-->
        <attr name="hint" format="string|reference" />  <!--与EditText的hint属性使用相同-->
        <attr name="hintTextColor" format="color" />    <!--与EditText的hintTextColor属性使用相同-->
        <attr name="srcClearButton" format="reference" />  <!--设置清除内容按钮的图片资源,与ImageView的src属性使用相同-->
        <attr name="srcPasswordDisplay" format="reference" /> <!--设置密码显示状态的图片资源,与ImageView的src属性使用相同-->
        <attr name="srcPasswordHidden" format="reference" /> <!--设置密码隐藏状态的图片资源,与ImageView的src属性使用相同-->
        <attr name="authCodeHint" format="string|reference" /> <!--设置获取验证码按钮显示的文字-->
        <attr name="inputTextColor" format="color" /> <!--设置输入文字的颜色-->
        <attr name="inputTextSize" format="dimension" /> <!--设置输入文字的大小,与EditText的textSize属性使用相同-->
        <attr name="authCodeTextColor" format="color" /> <!--设置获取验证码按钮文字颜色-->
        <attr name="authCodeTextSize" format="dimension" /> <!--设置获取验证码按钮文字大小-->
        <attr name="maxLength" format="integer" />  <!--设置可输入最大字符数-->
        <attr name="authCodeBackgroundColor" format="color" /> <!--获取验证码按钮背景色-->
        <attr name="authCodeBackgroundResource" format="reference" /> <!--获取验证码按钮背景资源文件-->
        <attr name="isShowAuthCodeBtn" format="boolean" />
        <attr name="editTextBackgroundColor" format="color" />
        <attr name="editTextBackgroundResource" format="reference" />
        <attr name="type" >
            <enum name="input" value="1"/>
            <enum name="option" value="2"/>
            <enum name="address" value="3"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="FloatViewLayout">
        <attr name="cacheXY" format="boolean" /> <!--是否缓存拖动贴边之后的位置，默认值为true-->
        <attr name="autoCalculateWelt" format="boolean" /> <!--是否自动贴边，默认值为true-->
        <attr name="animatorDuration" format="integer" /> <!--贴边动画时长，默认值250，单位毫秒-->
    </declare-styleable>

    <declare-styleable name="pickerview">
        <attr name="pickerview_gravity">
            <enum name="center" value="17" />
            <enum name="left" value="3" />
            <enum name="right" value="5" />
        </attr>
        <attr name="pickerview_textSize" format="dimension" />
        <attr name="pickerview_textColorOut" format="color" />
        <attr name="pickerview_textColorCenter" format="color" />
        <attr name="pickerview_dividerColor" format="color" />
        <attr name="pickerview_lineSpacingMultiplier" format="float" />
    </declare-styleable>

    <declare-styleable name="MZBannerView">
        <attr name="open_mz_mode" format="boolean" />
        <attr name="canLoop" format="boolean" />
        <attr name="indicatorPaddingLeft" format="dimension" />
        <attr name="indicatorPaddingRight" format="dimension" />
        <attr name="indicatorPaddingTop" format="dimension" />
        <attr name="indicatorPaddingBottom" format="dimension" />
        <attr name="indicatorAlign">
            <enum name="left" value="0" />
            <enum name="center" value="1" />
            <enum name="right" value="2" />
        </attr>
        <attr name="middle_page_cover" format="boolean" />
    </declare-styleable>

    <declare-styleable name="DragEditText">
        <attr name="textSize" format="dimension" />  <!--设置输入文字的大小,与EditText的textSize属性使用相同-->
        <attr name="text" format="string" />
        <attr name="textColor" format="color" />
        <attr name="hintText" format="string" /> <!--提示文字-->
        <attr name="hintColor" format="color" />
        <attr name="editDigits" format="string|reference" /> <!--定义可输入字符属性，与EditText的属性使用相同-->
        <attr name="backgroundResource" format="reference" />
        <attr name="editMaxLength" format="integer" />
        <attr name="lines" format="integer" />
        <attr name="gravity">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
            <enum name="top" value="2" />
            <enum name="bottom" value="3" />
            <enum name="center" value="4" />
            <enum name="center_horizontal" value="5" />
            <enum name="center_vertical" value="6" />
        </attr>
    </declare-styleable>

    <declare-styleable name="DragScaleView">
        <attr name="enabled_stroke" format="boolean" /> <!--是否启用描边，默认为false-->
        <attr name="stroke_color" format="color" /> <!--描边颜色，只有在启用描边的时候才生效-->
    </declare-styleable>

    <declare-styleable name="NavigationBar">
        <attr name="function">  <!--功能-->
            <flag name="NONE" value="0" />  <!--不显示任何功能-->
            <flag name="BACK_BUTTON" value="2" /> <!--导航栏返回按钮-->
            <flag name="LEFT_CLOSE_TITLE" value="4" /> <!--返回按钮右边的关闭文字按钮-->
            <flag name="LEFT_CLOSE_ICON" value="8" /> <!--返回按钮右边的 X 图标按钮，优先级高于文字关闭按钮-->
            <flag name="TITLE" value="16" /> <!--导航栏标题，优先级高于搜索-->
            <flag name="RIGHT_TEXT_BUTTON" value="32" /> <!--右边文字按钮-->
            <flag name="RIGHT_ICON_BUTTON" value="64" /> <!--右边图标按钮，优先级高于右边文字按钮-->
            <flag name="SEARCH" value="128" /> <!--中间的搜索功能，优先级低于标题-->
        </attr>
        <attr name="backButtonIconResource" format="reference" /> <!--返回按钮图标资源文件-->
        <attr name="closeButtonText" format="string" /> <!--关闭文字按钮文案-->
        <attr name="closeButtonTextColor" format="color" /> <!--关闭文字按钮文案颜色-->
        <attr name="closeButtonTextSize" format="dimension" /> <!--关闭文字按钮文案字体大小-->
        <attr name="closeButtonIconResource" format="reference" /> <!--关闭图标按钮图标资源文件-->
        <attr name="titleText" format="string" />
        <attr name="titleTextSize" format="dimension" />
        <attr name="titleTextColor" format="color" />
        <attr name="rightButtonText" format="string" />
        <attr name="rightButtonTextColor" format="color" />
        <attr name="rightButtonTextSize" format="dimension" />
        <attr name="rightButtonIconResource" format="reference" />
        <attr name="searchBackgroundResource" format="reference" />
        <attr name="searchHint" format="string" />
        <attr name="searchHintColor" format="color" />
        <attr name="searchTextSize" format="dimension" />
        <attr name="searchTextColor" format="color" />
        <attr name="searchEditEnable" format="boolean" />
        <attr name="searchEditFocusable" format="boolean" />
        <attr name="isImmersiveStatusBar" format="boolean" /> <!--是否沉浸式状态栏，默认值false-->
        <attr name="statusBarBackgroundColor" format="color" /> <!--沉浸式状态栏背景色，isImmersiveStatusBar=true时生效-->
    </declare-styleable>

    <declare-styleable name="ItemView">
        <attr name="itemLeftIcon" format="reference" />
        <attr name="itemRightIcon" format="reference" />
        <attr name="itemTitleText" format="string" />
        <attr name="itemTitleTextColor" format="color" />
        <attr name="itemTitleTextSize" format="dimension" />
        <attr name="itemRightText" format="string" />
        <attr name="itemRightTextColor" format="color" />
        <attr name="itemRightTextSize" format="dimension" />
        <attr name="rightContentIcon" format="reference" />
    </declare-styleable>

    <declare-styleable name="BottomNavigationView">
        <attr name="tab_select_text_color" format="color" />
        <attr name="tab_unselect_text_color" format="color" />
        <attr name="tab_text_size" format="dimension" />
    </declare-styleable>

    <declare-styleable name="CalendarView">
        <!--是否显示农历-->
        <attr name="show_lunar" format="boolean" />
        <!--是否显示上月和下月-->
        <attr name="show_last_next" format="boolean" />
        <!--是否显示节假日-->
        <attr name="show_holiday" format="boolean" />
        <!--是否显示节气-->
        <attr name="show_term" format="boolean" />
        <!--阳历的日期颜色-->
        <attr name="solar_color" format="color" />
        <!--阳历的日期尺寸-->
        <attr name="solar_size" format="integer" />
        <!--农历的日期颜色-->
        <attr name="lunar_color" format="color" />
        <!--农历的日期尺寸-->
        <attr name="lunar_size" format="integer" />
        <!--节日文字颜色-->
        <attr name="holiday_color" format="color" />
        <!--选中的日期文字颜色-->
        <attr name="choose_color" format="color" />
        <!--选中的日期背景-->
        <attr name="day_bg" format="reference" />
        <!--单选时切换月份，是否选中上次的日期-->
        <attr name="switch_choose" format="boolean" />
        <!--设置单选、多选-->
        <attr name="choose_type" format="enum">
            <enum name="single" value="0" />
            <enum name="multi" value="1" />
            <enum name="start_stop" value="2" />
        </attr>
        <!--   是否允许手动滑动     -->
        <attr name="manual_rolling" format="boolean" />
    </declare-styleable>

    <declare-styleable name="WeekView">
        <!--周的表示形式（x.x.x.x.x.x.x）-->
        <attr name="week_str" format="string" />
        <!--周的文字颜色-->
        <attr name="week_color" format="color" />
        <!--周的文字大小-->
        <attr name="week_size" format="integer" />
    </declare-styleable>

    <declare-styleable name="CameraTopRectView">
        <attr name="morientation" format="enum">
            <enum name="vertical" value="1" />
            <enum name="horizontal" value="2" />
        </attr>
    </declare-styleable>
</resources>